public class Solution {

   public static void main(String[] args) {
   }

   public static boolean task1(double x, double y
   ){
      return !(Math.abs(x) + Math.abs(y) <= 1);
   }

   public static boolean task3a(int A, int B)
   {
      return (A % 2 == 0) ^ (B % 2 == 0);
   }

   public static boolean task3b(int A, int B, int B)
   {
      return (A % 3 == 0)&&(B % 3 == 0)&&(C % 3 == 0);
   }

}
